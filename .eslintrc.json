{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "unicorn/no-null": "off",
        "unicorn/prevent-abbreviations": "off",
        "no-console": "off",
        "no-restricted-syntax": "off",
        "import/no-extraneous-dependencies": "off",
        "spaced-comment": "off",
        "comma-dangle": "off",
        "import/extensions": "off"
    }
}
